package Metamodel;

package Book {
  class Book {
    !unique !ordered attr String[1] title;
    val Chapter[*]#book chapters;
  }

  class Chapter {
    !unique !ordered attr String[1] title;
    !unique !ordered attr Integer[1] nbPages;
    !unique !ordered attr String[1] author;
    !ordered ref Book[1]#chapters book;
  }

}

package PrimitiveTypes {
  datatype Integer : "";

  datatype String : "";

  datatype Boolean : "";

}

