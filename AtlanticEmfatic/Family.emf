package Metamodel;

package Family {
  datatype String : "";

  datatype Integer : "";

  class Family {
    !unique !ordered attr String[1] name;
    !ordered ref Person[*] members;
  }

  class Person {
    !unique !ordered attr String[1] firstName;
    !ordered ref Person[1] closestFriend;
    !unique !ordered attr String[*] emailAddresses;
  }

}

