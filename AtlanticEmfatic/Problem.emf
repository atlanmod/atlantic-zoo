package Metamodel;

package Diagnostic {
  enum Severity {
    error = 0;
    warning = 0;
    critic = 0;
  }

  class Problem {
    !unique !ordered attr Severity[1] severity;
    !unique !ordered attr String[1] location;
    !unique !ordered attr String[1] description;
  }

}

package PrimitiveTypes {
  datatype String : "";

}

