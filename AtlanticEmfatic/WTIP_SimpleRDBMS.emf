package Metamodel;

package SimpleRDBMS {
  class Table {
    !unique !ordered attr String[1] name;
    !ordered val FKey[*] fkeys;
    !ordered ref Column[*] pkey;
    !ordered val Column[*] cols;
  }

  class Column {
    !unique !ordered attr String[1] name;
    !unique !ordered attr String[1] type;
  }

  class FKey {
    !ordered ref Table[1] references;
    !ordered ref Column[*] cols;
  }

}

package PrimitiveTypes {
  datatype String : "";

}

