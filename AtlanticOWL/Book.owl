<?xml version = '1.0' encoding = 'ISO-8859-1' ?>
<rdf:RDF xmlns:rdf = 'http://www.w3.org/1999/02/22-rdf-syntax-ns#' xmlns:rdfs = 'http://www.w3.org/2000/01/rdf-schema#' xmlns:xsd = 'http://www.w3.org/2001/XMLSchema#' xmlns:owl = 'http://www.w3.org/2002/07/owl#' xmlns = 'http://example.org/Book#' xml:base = 'http://example.org/Book'>
  <owl:Ontology rdf:about = 'Book'/>
  <owl:Class rdf:ID = 'Book'>
    <rdfs:label>Book</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Book.title'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Book.chapters'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Chapter'>
    <rdfs:label>Chapter</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Chapter.title'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Chapter.author'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Chapter.nbPages'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Chapter.book'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:DatatypeProperty rdf:ID = 'Chapter.author'>
    <rdfs:domain rdf:resource = '#Chapter'/>
    <rdfs:range rdf:resource = 'http://www.w3.org/2001/XMLSchema#string'/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID = 'Book.title'>
    <rdfs:domain rdf:resource = '#Book'/>
    <rdfs:range rdf:resource = 'http://www.w3.org/2001/XMLSchema#string'/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID = 'Chapter.nbPages'>
    <rdfs:domain rdf:resource = '#Chapter'/>
    <rdfs:range rdf:resource = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID = 'Chapter.title'>
    <rdfs:domain rdf:resource = '#Chapter'/>
    <rdfs:range rdf:resource = 'http://www.w3.org/2001/XMLSchema#string'/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:ID = 'Book.chapters'>
    <rdfs:domain rdf:resource = '#Book'/>
    <rdfs:range rdf:resource = '#Chapter'/>
    <owl:inverseOf rdf:resource = '#Chapter.book'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Chapter.book'>
    <rdfs:domain rdf:resource = '#Chapter'/>
    <rdfs:range rdf:resource = '#Book'/>
    <owl:inverseOf rdf:resource = '#Book.chapters'/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about = '#Chapter.nbPages'/>
  <owl:FunctionalProperty rdf:about = '#Chapter.book'/>
  <owl:FunctionalProperty rdf:about = '#Book.title'/>
  <owl:FunctionalProperty rdf:about = '#Chapter.author'/>
  <owl:FunctionalProperty rdf:about = '#Chapter.title'/>
</rdf:RDF>
