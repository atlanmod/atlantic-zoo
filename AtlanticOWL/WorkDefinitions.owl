<?xml version = '1.0' encoding = 'ISO-8859-1' ?>
<rdf:RDF xmlns:rdf = 'http://www.w3.org/1999/02/22-rdf-syntax-ns#' xmlns:rdfs = 'http://www.w3.org/2000/01/rdf-schema#' xmlns:xsd = 'http://www.w3.org/2001/XMLSchema#' xmlns:owl = 'http://www.w3.org/2002/07/owl#' xmlns = 'http://example.org/WorkDefinitions#' xml:base = 'http://example.org/WorkDefinitions'>
  <owl:Ontology rdf:about = 'WorkDefinitions'/>
  <owl:Class rdf:ID = 'Iteration'>
    <rdfs:label>Iteration</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#WorkDefinition'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Iteration.phase'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Iteration.activities'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'ProcessPerformer'>
    <rdfs:label>ProcessPerformer</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#ProcessPerformer.features'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'WorkProduct'>
    <rdfs:label>WorkProduct</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#WorkProduct.responsible'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Role'>
    <rdfs:label>Role</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#ProcessPerformer'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Role.products'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'WorkDefinition'>
    <rdfs:label>WorkDefinition</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#WorkDefinition.parentWorks'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#WorkDefinition.subWorks'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#WorkDefinition.owner'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Step'>
    <rdfs:label>Step</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID = 'Phase'>
    <rdfs:label>Phase</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#WorkDefinition'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Phase.iterations'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Phase.lifeCycle'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Lifecycle'>
    <rdfs:label>Lifecycle</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#WorkDefinition'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Lifecycle.phases'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Activity'>
    <rdfs:label>Activity</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#WorkDefinition'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Activity.iteration'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Activity.steps'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:ID = 'Activity.iteration'>
    <rdfs:domain rdf:resource = '#Activity'/>
    <rdfs:range rdf:resource = '#Iteration'/>
    <owl:inverseOf rdf:resource = '#Iteration.activities'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Iteration.activities'>
    <rdfs:domain rdf:resource = '#Iteration'/>
    <rdfs:range rdf:resource = '#Activity'/>
    <owl:inverseOf rdf:resource = '#Activity.iteration'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Iteration.phase'>
    <rdfs:domain rdf:resource = '#Iteration'/>
    <rdfs:range rdf:resource = '#Phase'/>
    <owl:inverseOf rdf:resource = '#Phase.iterations'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Phase.lifeCycle'>
    <rdfs:domain rdf:resource = '#Phase'/>
    <rdfs:range rdf:resource = '#Lifecycle'/>
    <owl:inverseOf rdf:resource = '#Lifecycle.phases'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'WorkDefinition.subWorks'>
    <rdfs:domain rdf:resource = '#WorkDefinition'/>
    <rdfs:range rdf:resource = '#WorkDefinition'/>
    <owl:inverseOf rdf:resource = '#WorkDefinition.parentWorks'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Lifecycle.phases'>
    <rdfs:domain rdf:resource = '#Lifecycle'/>
    <rdfs:range rdf:resource = '#Phase'/>
    <owl:inverseOf rdf:resource = '#Phase.lifeCycle'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'WorkDefinition.parentWorks'>
    <rdfs:domain rdf:resource = '#WorkDefinition'/>
    <rdfs:range rdf:resource = '#WorkDefinition'/>
    <owl:inverseOf rdf:resource = '#WorkDefinition.subWorks'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'WorkDefinition.owner'>
    <rdfs:domain rdf:resource = '#WorkDefinition'/>
    <rdfs:range rdf:resource = '#ProcessPerformer'/>
    <owl:inverseOf rdf:resource = '#ProcessPerformer.features'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'ProcessPerformer.features'>
    <rdfs:domain rdf:resource = '#ProcessPerformer'/>
    <rdfs:range rdf:resource = '#WorkDefinition'/>
    <owl:inverseOf rdf:resource = '#WorkDefinition.owner'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'WorkProduct.responsible'>
    <rdfs:domain rdf:resource = '#WorkProduct'/>
    <rdfs:range rdf:resource = '#Role'/>
    <owl:inverseOf rdf:resource = '#Role.products'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Activity.steps'>
    <rdfs:domain rdf:resource = '#Activity'/>
    <rdfs:range rdf:resource = '#Step'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Role.products'>
    <rdfs:domain rdf:resource = '#Role'/>
    <rdfs:range rdf:resource = '#WorkProduct'/>
    <owl:inverseOf rdf:resource = '#WorkProduct.responsible'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Phase.iterations'>
    <rdfs:domain rdf:resource = '#Phase'/>
    <rdfs:range rdf:resource = '#Iteration'/>
    <owl:inverseOf rdf:resource = '#Iteration.phase'/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about = '#WorkProduct.responsible'/>
  <owl:FunctionalProperty rdf:about = '#Iteration.phase'/>
  <owl:FunctionalProperty rdf:about = '#WorkDefinition.owner'/>
  <owl:FunctionalProperty rdf:about = '#Phase.lifeCycle'/>
  <owl:FunctionalProperty rdf:about = '#Activity.iteration'/>
  <owl:SymmetricProperty rdf:about = '#WorkDefinition.subWorks'/>
  <owl:SymmetricProperty rdf:about = '#WorkDefinition.parentWorks'/>
</rdf:RDF>
