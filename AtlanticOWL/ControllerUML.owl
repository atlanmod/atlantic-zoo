<?xml version = '1.0' encoding = 'ISO-8859-1' ?>
<rdf:RDF xmlns:rdf = 'http://www.w3.org/1999/02/22-rdf-syntax-ns#' xmlns:rdfs = 'http://www.w3.org/2000/01/rdf-schema#' xmlns:xsd = 'http://www.w3.org/2001/XMLSchema#' xmlns:owl = 'http://www.w3.org/2002/07/owl#' xmlns = 'http://example.org/ControllerUML#' xml:base = 'http://example.org/ControllerUML'>
  <owl:Ontology rdf:about = 'ControllerUML'/>
  <owl:Class rdf:ID = 'ViewState'>
    <rdfs:label>ViewState</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#State'/>
  </owl:Class>
  <owl:Class rdf:ID = 'Controller'>
    <rdfs:label>Controller</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Controller.controllerAttribute'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Controller.behavior'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'State'>
    <rdfs:label>State</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#State.incoming'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#State.substates'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#State.outGoing'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#State.theContainer'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#State.theContainer'/>
        <owl:maxCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:maxCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'StateTransition'>
    <rdfs:label>StateTransition</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#StateTransition.trigger'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#StateTransition.source'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#StateTransition.effect'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#StateTransition.effect'/>
        <owl:maxCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:maxCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#StateTransition.target'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'StateMachine'>
    <rdfs:label>StateMachine</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#StateMachine.states'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'StateMachineAction'>
    <rdfs:label>StateMachineAction</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID = 'Event'>
    <rdfs:label>Event</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID = 'SubControllerState'>
    <rdfs:label>SubControllerState</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#State'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#SubControllerState.controller'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'ControllerAttribute'>
    <rdfs:label>ControllerAttribute</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#ControllerAttribute.controller'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:ID = 'Controller.behavior'>
    <rdfs:domain rdf:resource = '#Controller'/>
    <rdfs:range rdf:resource = '#StateMachine'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'State.incoming'>
    <rdfs:domain rdf:resource = '#State'/>
    <rdfs:range rdf:resource = '#StateTransition'/>
    <owl:inverseOf rdf:resource = '#StateTransition.target'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'ControllerAttribute.controller'>
    <rdfs:domain rdf:resource = '#ControllerAttribute'/>
    <rdfs:range rdf:resource = '#Controller'/>
    <owl:inverseOf rdf:resource = '#Controller.controllerAttribute'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Controller.controllerAttribute'>
    <rdfs:domain rdf:resource = '#Controller'/>
    <rdfs:range rdf:resource = '#ControllerAttribute'/>
    <owl:inverseOf rdf:resource = '#ControllerAttribute.controller'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'StateMachine.states'>
    <rdfs:domain rdf:resource = '#StateMachine'/>
    <rdfs:range rdf:resource = '#State'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'StateTransition.target'>
    <rdfs:domain rdf:resource = '#StateTransition'/>
    <rdfs:range rdf:resource = '#State'/>
    <owl:inverseOf rdf:resource = '#State.incoming'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'StateTransition.trigger'>
    <rdfs:domain rdf:resource = '#StateTransition'/>
    <rdfs:range rdf:resource = '#Event'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'State.substates'>
    <rdfs:domain rdf:resource = '#State'/>
    <rdfs:range rdf:resource = '#State'/>
    <owl:inverseOf rdf:resource = '#State.theContainer'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'StateTransition.effect'>
    <rdfs:domain rdf:resource = '#StateTransition'/>
    <rdfs:range rdf:resource = '#StateMachineAction'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'State.theContainer'>
    <rdfs:domain rdf:resource = '#State'/>
    <rdfs:range rdf:resource = '#State'/>
    <owl:inverseOf rdf:resource = '#State.substates'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'SubControllerState.controller'>
    <rdfs:domain rdf:resource = '#SubControllerState'/>
    <rdfs:range rdf:resource = '#Controller'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'StateTransition.source'>
    <rdfs:domain rdf:resource = '#StateTransition'/>
    <rdfs:range rdf:resource = '#State'/>
    <owl:inverseOf rdf:resource = '#State.outGoing'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'State.outGoing'>
    <rdfs:domain rdf:resource = '#State'/>
    <rdfs:range rdf:resource = '#StateTransition'/>
    <owl:inverseOf rdf:resource = '#StateTransition.source'/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about = '#StateTransition.target'/>
  <owl:FunctionalProperty rdf:about = '#Controller.behavior'/>
  <owl:FunctionalProperty rdf:about = '#ControllerAttribute.controller'/>
  <owl:FunctionalProperty rdf:about = '#StateTransition.source'/>
  <owl:FunctionalProperty rdf:about = '#SubControllerState.controller'/>
  <owl:FunctionalProperty rdf:about = '#StateTransition.trigger'/>
  <owl:SymmetricProperty rdf:about = '#State.substates'/>
  <owl:SymmetricProperty rdf:about = '#State.theContainer'/>
</rdf:RDF>
