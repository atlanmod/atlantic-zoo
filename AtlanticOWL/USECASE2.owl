<?xml version = '1.0' encoding = 'ISO-8859-1' ?>
<rdf:RDF xmlns:rdf = 'http://www.w3.org/1999/02/22-rdf-syntax-ns#' xmlns:rdfs = 'http://www.w3.org/2000/01/rdf-schema#' xmlns:xsd = 'http://www.w3.org/2001/XMLSchema#' xmlns:owl = 'http://www.w3.org/2002/07/owl#' xmlns = 'http://example.org/USECASEUML#' xml:base = 'http://example.org/USECASEUML'>
  <owl:Ontology rdf:about = 'USECASEUML'/>
  <owl:Class rdf:ID = 'Role'>
    <rdfs:label>Role</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Role.interacted_with'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'NonFunctionnelRequirement'>
    <rdfs:label>NonFunctionnelRequirement</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#Requirement'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#NonFunctionnelRequirement.scopes'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'ScenarioDescription'>
    <rdfs:label>ScenarioDescription</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#ScenarioDescription.useCase'/>
        <owl:cardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:cardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Post'>
    <rdfs:label>Post</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#Condition'/>
  </owl:Class>
  <owl:Class rdf:ID = 'EventRole'>
    <rdfs:label>EventRole</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#Role'/>
  </owl:Class>
  <owl:Class rdf:ID = 'Pre'>
    <rdfs:label>Pre</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#Condition'/>
  </owl:Class>
  <owl:Class rdf:ID = 'Resource'>
    <rdfs:label>Resource</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID = 'SystemRole'>
    <rdfs:label>SystemRole</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#Role'/>
  </owl:Class>
  <owl:Class rdf:ID = 'FunctionnelRequirement'>
    <rdfs:label>FunctionnelRequirement</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#Requirement'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#FunctionnelRequirement.specifies'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'UseCase'>
    <rdfs:label>UseCase</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#UseCase.condition'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#UseCase.scoped_by'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#UseCase.specified_by'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#UseCase.scenario'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#UseCase.meet_by'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#UseCase.interacter'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Condition'>
    <rdfs:label>Condition</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Condition.constrained_with'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'HumanRole'>
    <rdfs:label>HumanRole</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#Role'/>
  </owl:Class>
  <owl:Class rdf:ID = 'Manage'>
    <rdfs:label>Manage</rdfs:label>
    <rdfs:subClassOf rdf:resource = '#UseCase'/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Manage.managed_Resource'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>0</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID = 'Requirement'>
    <rdfs:label>Requirement</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID = 'Goal'>
    <rdfs:label>Goal</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource = '#Goal.to_meet'/>
        <owl:minCardinality  rdf:datatype = 'http://www.w3.org/2001/XMLSchema#nonNegativeInteger'>1</owl:minCardinality >
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:ID = 'Manage.managed_Resource'>
    <rdfs:domain rdf:resource = '#Manage'/>
    <rdfs:range rdf:resource = '#Resource'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'UseCase.meet_by'>
    <rdfs:domain rdf:resource = '#UseCase'/>
    <rdfs:range rdf:resource = '#Goal'/>
    <owl:inverseOf rdf:resource = '#Goal.to_meet'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'UseCase.condition'>
    <rdfs:domain rdf:resource = '#UseCase'/>
    <rdfs:range rdf:resource = '#Condition'/>
    <owl:inverseOf rdf:resource = '#Condition.constrained_with'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'UseCase.scoped_by'>
    <rdfs:domain rdf:resource = '#UseCase'/>
    <rdfs:range rdf:resource = '#NonFunctionnelRequirement'/>
    <owl:inverseOf rdf:resource = '#NonFunctionnelRequirement.scopes'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'ScenarioDescription.useCase'>
    <rdfs:domain rdf:resource = '#ScenarioDescription'/>
    <rdfs:range rdf:resource = '#UseCase'/>
    <owl:inverseOf rdf:resource = '#UseCase.scenario'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'UseCase.scenario'>
    <rdfs:domain rdf:resource = '#UseCase'/>
    <rdfs:range rdf:resource = '#ScenarioDescription'/>
    <owl:inverseOf rdf:resource = '#ScenarioDescription.useCase'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Condition.constrained_with'>
    <rdfs:domain rdf:resource = '#Condition'/>
    <rdfs:range rdf:resource = '#UseCase'/>
    <owl:inverseOf rdf:resource = '#UseCase.condition'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'FunctionnelRequirement.specifies'>
    <rdfs:domain rdf:resource = '#FunctionnelRequirement'/>
    <rdfs:range rdf:resource = '#UseCase'/>
    <owl:inverseOf rdf:resource = '#UseCase.specified_by'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'UseCase.interacter'>
    <rdfs:domain rdf:resource = '#UseCase'/>
    <rdfs:range rdf:resource = '#Role'/>
    <owl:inverseOf rdf:resource = '#Role.interacted_with'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Goal.to_meet'>
    <rdfs:domain rdf:resource = '#Goal'/>
    <rdfs:range rdf:resource = '#UseCase'/>
    <owl:inverseOf rdf:resource = '#UseCase.meet_by'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'Role.interacted_with'>
    <rdfs:domain rdf:resource = '#Role'/>
    <rdfs:range rdf:resource = '#UseCase'/>
    <owl:inverseOf rdf:resource = '#UseCase.interacter'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'NonFunctionnelRequirement.scopes'>
    <rdfs:domain rdf:resource = '#NonFunctionnelRequirement'/>
    <rdfs:range rdf:resource = '#UseCase'/>
    <owl:inverseOf rdf:resource = '#UseCase.scoped_by'/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID = 'UseCase.specified_by'>
    <rdfs:domain rdf:resource = '#UseCase'/>
    <rdfs:range rdf:resource = '#FunctionnelRequirement'/>
    <owl:inverseOf rdf:resource = '#FunctionnelRequirement.specifies'/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about = '#ScenarioDescription.useCase'/>
</rdf:RDF>
