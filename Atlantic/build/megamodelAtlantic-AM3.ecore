  <megamodelElements xsi:type="am3:Metamodel" location="472:1-473:14" uri="Promenade.km3" name="Promenade" version="1.0" domains="" authors="Youssef Srour (Srour.youssef_NOSPAM &lt;AT> gmail.com)" date="2006/07/19" description="This metamodel describes Promenade. It is a PML which intends to formalize the construction of SPMs." see="http://delivery.acm.org/10.1145/570000/568839/p457-ribo.pdf?key1=568839&amp;key2=0221904511&amp;coll=ACM&amp;dl=ACM&amp;CFID=15151515&amp;CFTOKEN=6184618&#xA;" locationType="workspace"/>
