-- @name		Problem
-- @version		1.0
-- @domains		
-- @authors		Frédéric Jouault
-- @date		2005/06/01
-- @description	This metamodel describes the notion of problem created after a diagnostic
-- @see			

package Diagnostic {

	enumeration Severity {
		literal error;
		literal warning;
		literal critic;
	}

	class Problem {
		attribute severity: Severity;
		attribute location: String;
		attribute description: String;
	}
}

package PrimitiveTypes {
	datatype String;
}
