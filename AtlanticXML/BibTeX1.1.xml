<?xml version = '1.0' encoding = 'ISO-8859-1' ?>
<Metamodel>
  <Package name = 'BibTeX'>
    <Class name = 'BibTeXFile' isAbstract = 'false' supertypes = ''>
      <Reference name = 'entries' lower = '0' upper = '-1' isOrdered = 'false' isContainer = 'true' type = 'BibTeX::BibTeXEntry' opposite = ''/>
    </Class>
    <Class name = 'Author' isAbstract = 'false' supertypes = ''>
      <Attribute name = 'author' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'BibTeXEntry' isAbstract = 'true' supertypes = ''>
      <Attribute name = 'id' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'AuthoredEntry' isAbstract = 'true' supertypes = 'BibTeX::BibTeXEntry'>
      <Reference name = 'authors' lower = '1' upper = '-1' isOrdered = 'false' isContainer = 'true' type = 'BibTeX::Author' opposite = ''/>
    </Class>
    <Class name = 'DatedEntry' isAbstract = 'true' supertypes = 'BibTeX::BibTeXEntry'>
      <Attribute name = 'year' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'TitledEntry' isAbstract = 'true' supertypes = 'BibTeX::BibTeXEntry'>
      <Attribute name = 'title' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'BookTitledEntry' isAbstract = 'true' supertypes = 'BibTeX::BibTeXEntry'>
      <Attribute name = 'booktitle' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'Article' isAbstract = 'false' supertypes = 'BibTeX::AuthoredEntry, BibTeX::DatedEntry, BibTeX::TitledEntry'>
      <Attribute name = 'journal' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'TechReport' isAbstract = 'false' supertypes = 'BibTeX::AuthoredEntry, BibTeX::DatedEntry, BibTeX::TitledEntry'/>
    <Class name = 'Unpublished' isAbstract = 'false' supertypes = 'BibTeX::AuthoredEntry, BibTeX::TitledEntry'>
      <Attribute name = 'note' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'Manual' isAbstract = 'false' supertypes = 'BibTeX::TitledEntry'/>
    <Class name = 'Proceedings' isAbstract = 'false' supertypes = 'BibTeX::DatedEntry, BibTeX::TitledEntry'/>
    <Class name = 'InProceedings' isAbstract = 'false' supertypes = 'BibTeX::Proceedings, BibTeX::AuthoredEntry, BibTeX::BookTitledEntry'/>
    <Class name = 'Booklet' isAbstract = 'false' supertypes = 'BibTeX::DatedEntry'/>
    <Class name = 'Book' isAbstract = 'false' supertypes = 'BibTeX::AuthoredEntry, BibTeX::DatedEntry, BibTeX::TitledEntry'>
      <Attribute name = 'publisher' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'InCollection' isAbstract = 'false' supertypes = 'BibTeX::Book, BibTeX::BookTitledEntry'/>
    <Class name = 'InBook' isAbstract = 'false' supertypes = 'BibTeX::Book'>
      <Attribute name = 'chapter' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::Integer'/>
    </Class>
    <Class name = 'Misc' isAbstract = 'false' supertypes = 'BibTeX::BibTeXEntry'/>
    <Class name = 'ThesisEntry' isAbstract = 'true' supertypes = 'BibTeX::AuthoredEntry, BibTeX::DatedEntry, BibTeX::TitledEntry'>
      <Attribute name = 'school' lower = '1' upper = '1' isOrdered = 'false' isUnique = 'false' type = 'PrimitiveTypes::String'/>
    </Class>
    <Class name = 'PhDThesis' isAbstract = 'false' supertypes = 'BibTeX::ThesisEntry'/>
    <Class name = 'MasterThesis' isAbstract = 'false' supertypes = 'BibTeX::ThesisEntry'/>
  </Package>
  <Package name = 'PrimitiveTypes'>
    <DataType name = 'String'/>
    <DataType name = 'Integer'/>
  </Package>
</Metamodel>
